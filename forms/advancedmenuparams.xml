					description="PLG_ADVANCEDMENUPARAMS_FIELD_ACCESSKEY_DESC" 
					size="10" />
						
		      <field name="note01" type="note" label="PLG_ADVANCEDMENUPARAMS_NOTE1_LABEL" description="PLG_ADVANCEDMENUPARAMS_NOTE1_DESC" class="alert" close="true" />				
			  
